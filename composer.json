{
    "name": "mvqn/ucrm-plugin-sdk",
    "description": "An alternative UCRM SDK designed to ease Plugin development.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": ">=7.1.26",

        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-pdo": "*",

        "mvqn/data": "1.*",
        "mvqn/dynamics": "1.*",
        "mvqn/rest": "1.*",

        "nette/php-generator": "~3.2",
        "defuse/php-encryption": "~2.2",

        "phpunit/phpunit": "~7",
        "vlucas/phpdotenv": "~2.5"
    },
    "require-dev": {
        "php": ">=7.2.13"
    },
    "autoload": {
        "psr-4": {
            "UCRM\\": "src/UCRM/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UCRM\\": "tests/UCRM/"
        }
    }
}
